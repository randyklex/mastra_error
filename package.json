{
  "name": "devyze-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "prisma generate && next dev --turbopack",
    "dev:old": "prisma generate && next dev",
    "inngest": "npx inngest-cli@latest dev --no-discovery -u http://localhost:3000/api/integrations/inngest",
    "build": "prisma generate && prisma migrate deploy && next build",
    "compile": "prisma generate && next build",
    "typecheck": "prisma generate && tsc",
    "typecheck-native": "prisma generate && tsgo",
    "format:write": "prettier --write \"**/*.{css,js,json,jsx,ts,tsx}\"",
    "format": "prettier \"**/*.{css,js,json,jsx,ts,tsx}\"",
    "start": "next start",
    "lint": "next lint",
    "dev:turbopack": "prisma generate && next dev --turbo",
    "cli": "cross-env NODE_ENV=cli npx tsx --require ./lib/cli/setup.js lib/cli/index.ts",
    "dev:mastra": "mastra dev --dir lib/mastra",
    "build:mastra": "mastra build --dir lib/mastra"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.52",
    "@ai-sdk/react": "^2.0.68",
    "@azure/communication-email": "^1.0.0",
    "@azure/monitor-opentelemetry-exporter": "1.0.0-beta.27",
    "@azure/storage-blob": "^12.24.0",
    "@base-ui-components/react": "^1.0.0-beta.4",
    "@clerk/nextjs": "^6.23.3",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@floating-ui/dom": "^1.7.3",
    "@floating-ui/react": "^0.27.15",
    "@frsource/autoresize-textarea": "^2.0.96",
    "@hocuspocus/provider": "^2.15.3",
    "@hookform/resolvers": "^5.0.1",
    "@inngest/realtime": "^0.3.2",
    "@langchain/openai": "^0.0.34",
    "@langchain/pinecone": "^0.1.3",
    "@mastra/ai-sdk": "^0.1.1",
    "@mastra/core": "^0.20.1",
    "@mastra/libsql": "^0.15.1",
    "@mastra/loggers": "^0.10.15",
    "@mastra/memory": "^0.15.6",
    "@mastra/pg": "^0.17.2"
  }
}
